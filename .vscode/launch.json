            }
        },
        {
            "name": "geocode sample",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/src/api/bin/Debug/netcoreapp2.2/api.mapserv.utah.gov.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/api",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}/api/v1/geocode/326 easth south temple st/slc?apikey=AGRC-Explorer",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start ${auto-detect-url}"
