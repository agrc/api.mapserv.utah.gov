{
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode",
    "editor.wordWrapColumn": 120
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true
  },
  "coverage-gutters.highlightdark": "rgb(61, 153, 112, .05)",
  "coverage-gutters.noHighlightDark": "rgb(255, 65, 54, .05)",
  "coverage-gutters.partialHighlightDark": "rgb(255, 133, 27, .05)",
  "coverage-gutters.showGutterCoverage": false,
  "coverage-gutters.showLineCoverage": true,
  "coverage-gutters.showRulerCoverage": false,
  "cSpell.enableFiletypes": [
    "astro",
    "markdown",
    "mdx"
  ],
  "cSpell.words": [
    "accesstoken",
    "addnum",
    "addresspoints",
    "ADDRSYS",
    "agrc",
    "ALRS",
    "alternatename",
    "apikey",
    "appsettings",
    "arcgis",
    "arcpy",
    "aspnetcore",
    "astro",
    "astrojs",
    "atlnamesaddrpnts",
    "atlnamesroads",
    "autodocs",
    "Autofac",
    "Buildx",
    "cadastre",
    "centerlines",
    "cloudrun",
    "clsx",
    "codecov",
    "Concurrencies",
    "Datasource",
    "Errorable",
    "esrijson",
    "evenodd",
    "extralight",
    "fastenshtein",
    "Firestore",
    "FROMADDR",
    "fullhuman",
    "gcloud",
    "geocode",
    "geocoded",
    "geocoder",
    "geocoderoads",
    "geocoders",
    "Geocodes",
    "Geolocators",
    "geoscience",
    "geospatial",
    "GLOBALID",
    "gserviceaccount",
    "heroicons",
    "hookform",
    "immer",
    "jquery",
    "kube",
    "kubectl",
    "kubernetes",
    "Kustomize",
    "labelledby",
    "lcub",
    "levelname",
    "Linkables",
    "loadbalancer",
    "mapserv",
    "mdash",
    "mediatr",
    "midvale",
    "multipoint",
    "multipolygon",
    "Newtonsoft",
    "noopener",
    "noreferrer",
    "nowrap",
    "npgsql",
    "oidc",
    "openapi",
    "opensgid",
    "packagejson",
    "pbcopy",
    "pgdata",
    "Pnts",
    "pobox",
    "poboxes",
    "pointfromtext",
    "polyline",
    "POSTDIR",
    "Postgre",
    "postgres",
    "postgresql",
    "POSTTYPE",
    "PREDIR",
    "Projectable",
    "psql",
    "purgecss",
    "randh",
    "reactfire",
    "reproject",
    "reprojecting",
    "requestip",
    "reversemilepost",
    "rijndael",
    "roadcenterlines",
    "roadgrinder",
    "semibold",
    "serilog",
    "sgid",
    "shouldly",
    "Signin",
    "smocker",
    "srid",
    "streetzone",
    "Suggestable",
    "Swashbuckle",
    "tablename",
    "tailwind",
    "tailwindcss",
    "tanstack",
    "tbody",
    "thead",
    "TOADDR",
    "udot",
    "UGRC",
    "utahdts",
    "Utahid",
    "VITE",
    "webapi",
    "Weighable",
    "wkid",
    "wkids",
    "worknote",
    "xunit"
  ],
  "dotnet.defaultSolution": "api.mapserv.utah.gov.sln",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.rulers": [
    120
  ],
  "editor.wordWrap": "on",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".editorconfig": ".codecov.yml, omnisharp.json",
    "*.cts": "${capture}.typegen.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts, ${capture}.test.js",
    "*.json": "${capture}.json, ${capture}.*.json",
    "*.jsx": "${capture}.js, ${capture}.stories.jsx, ${capture}.test.jsx",
    "*.mts": "${capture}.typegen.ts",
    "*.sln": "Directory.Build.props, Directory.Build.targets",
    "*.ts": "${capture}.js, ${capture}.typegen.ts",
    "*.tsx": "${capture}.ts, ${capture}.typegen.ts",
    "docker-compose.yml": "docker-compose.override.yml, .dockerignore",
    "firebase.json": ".firebaserc, firestore.indexes.json, firestore.rules, storage.rules, firebase-debug.log, firestore-debug.log, ui-debug.log",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    "Program.cs": "Globalusings.cs",
    "README.md": "LICENSE, .gitattributes, .gitignore",
    "release-please-config.json": ".release-please-manifest.json",
    "tailwind.config.js": "postcss.config.js",
    "tsconfig.json": "tsconfig.*.json"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "omnisharp.enableEditorConfigSupport": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "prettier.documentSelectors": [
    "*.{js,jsx,css,html,md,json}"
  ],
  "python.formatting.provider": "none",
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.pycodestyleEnabled": false,
  "python.testing.pytestArgs": [
    "--no-cov"
  ],
  "python.testing.pytestEnabled": true,
  "tailwindCSS.experimental.classRegex": [
    [
      "clsx\\(([^)]*)\\)",
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ]
  ]
}
