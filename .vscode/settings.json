{
  "dotnet-test-explorer.testProjectPath": "test/**/*.csproj",
  "dotnet-test-explorer.testArguments": "/p:CollectCoverage=true /p:CoverletOutputFormat=lcov /p:CoverletOutput=../../lcov.info",
  "cSpell.words": [
    "agrc",
    "arcgis",
    "codecov",
    "esrijson",
    "evenodd",
    "fastenshtein",
    "fullhuman",
    "gcloud",
    "immer",
    "kube",
    "kubectl",
    "kubernetes",
    "mapserv",
    "mediatr",
    "npgsql",
    "pgdata",
    "pobox",
    "purgecss",
    "reproject",
    "reprojecting",
    "semibold",
    "serilog",
    "sgid",
    "shouldly",
    "tailwind",
    "tailwindcss",
    "webapi",
    "wkid",
    "wkids",
    "xunit"
  ],
  "editor.rulers": [120],
  "coverage-gutters.showGutterCoverage": false,
  "coverage-gutters.showLineCoverage": true,
  "coverage-gutters.showRulerCoverage": false,
  "coverage-gutters.highlightdark": "rgb(61, 153, 112, .05)",
  "coverage-gutters.noHighlightDark": "rgb(255, 65, 54, .05)",
  "coverage-gutters.partialHighlightDark": "rgb(255, 133, 27, .05)",
  "coverage-gutters.xmlname": "coverage.xml"
}
