@model WebAPI.API.Models.ViewModels.MainView
<div class="well">
    <span class="label label-verb label-get pull-left">Get</span> <span class="label label-url url pull-right">
                                                                      /info/FieldNames/:featureClass</span>
    <div class="clearfix">
    </div>
    <h3>
        Feature Class Attribute Names
    </h3>
    <p>
        Returns a list of attribute names for the feature class.
    </p>
    <button class="btn btn-default" data-details="true">
        Details
    </button>
    <span class="help-inline label label-success label-status pull-right">implemented</span>
    <div class="clearfix">
    </div>
    <form class="form-horizontal" style="display: none">
        <fieldset>
            <legend class="hidden"></legend>
            <div class="form-group">
                <label class="col-sm-2 control-label" for="featureClass">
                    featureClass
                </label>
                <div class="col-sm-8">
                    <input type="text" id="featureClass" name="featureClass" class="form-control" data-required/>
                    <span class="help-inline label label-info">string</span> <span class="help-inline label label-danger">
                                                                                 required</span>
                    <p class="help-block">
                        Feature class name: county_boundaries.
                    </p>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-2 control-label" for="category">
                    category
                </label>
                <div class="col-sm-8">
                    <input type="text" id="category" name="category" class="form-control"/>
                    <span class="help-inline label label-info">string</span> <span class="help-inline label label-success">
                                                                                 optional</span>
                    <p class="help-block">
                        Feature category: boundaries. This should be provided if there are multiple table name sharing the same name in different categories. It is best practice to use this field.
                    </p>
                </div>
            </div>
        </fieldset>
        <div class="progress-striped active col-sm-offset-2 col-sm-8 col-xs-12">
            <button class="btn btn-primary col-sm-offset-2 col-sm-8 col-xs-12" data-demo data-url="@HttpUtility.UrlDecode(Url.RouteUrl("v1_InfoAttributesApi", new
                                                                                                   {
                                                                                                       httproute = "", controller = "Info", version = "1", featureClass = ":featureClass"
                                                                                                   }))">
                Try it
            </button>
        </div>
        <div class="code-well">
            <a class="close" data-close>×</a>
            <div class="clearfix">
            </div>
            <pre class="pre-scrollable-small" data-codeContainer>Code</pre>
            <div class="alert alert-info text-center" id="sampleUrl" style="display: none">
            </div>
        </div>
    </form>
</div>