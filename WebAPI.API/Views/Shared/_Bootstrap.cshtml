<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <title>@ViewBag.Title</title>
        <link rel="shortcut icon" href="//gis.utah.gov/images/favicon.ico">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        @Scripts.Render("~/bundles/jquery")
        @Styles.Render("~/content/bundles")
    </head>
    <body>
        <div id="wrap">
            <div class="navbar navbar-fixed-top">
                <div class="navbar-inner">
                    <div class="container">
                        @Html.ActionLink("AGRC Web API", "Index", new
                            {
                                Controller = "Home"
                            }, new
                                {
                                    @class = "brand"
                                })
                    </div>
                </div>
            </div>
            <div class="container">
                @RenderBody()
                @Scripts.Render("~/bundles/js")
                @Scripts.Render("~/bundles/bootstrap")
            </div>  
            <div id="push"></div>      
        </div>
        <div id="footer">
            <div class="container">                
                @Html.Partial("Footer")
            </div>
        </div>
    </body>
</html>