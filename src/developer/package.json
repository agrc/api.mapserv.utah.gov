{
  "name": "@ugrc/developer",
  "version": "2.8.1",
  "private": true,
  "description": "The API key self service website for the UGRC API.",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:storybook": "storybook build --output-dir ./dist/storybook",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:firebase\" \"wait-on http://127.0.0.1:4000 && npm run dev:vite\"",
    "dev:firebase": "firebase emulators:start --import ../../data/firebase --only auth,firestore,functions",
    "dev:vite": "vite",
    "firebase:migrate:dev": "fireway migrate --projectId ut-dts-agrc-web-api-dev",
    "firebase:migrate:local": "FIRESTORE_EMULATOR_HOST='127.0.0.1:8080' fireway migrate --projectId ut-dts-agrc-web-api-dev",
    "firebase:migrate:prod": "fireway migrate --projectId ut-dts-agrc-web-api-prod",
    "firebase:save-state": "firebase emulators:start --import ../../data/firebase --export-on-exit --only auth,firestore,functions",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint ./ --ext .js,.jsx --quiet --fix --ignore-path ./.eslintignore",
    "preview": "vite preview",
    "start": "npm run dev",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --silent --coverage --ui --open"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-table": "^8.21.3",
    "@ugrc/utah-design-system": "^1.34.2",
    "@vitejs/plugin-react": "^5.1.2",
    "clsx": "^2.1.1",
    "firebase": "^12.8.0",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^7.0.0",
    "md5": "^2.3.0",
    "react": "^19.2.3",
    "react-aria-components": "^1.14.0",
    "react-copy-to-clipboard-ts": "^1.3.1",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.3",
    "react-hook-form": "^7.71.1",
    "react-router": "^7.12.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.2.4",
    "@storybook/addon-links": "^10.2.4",
    "@storybook/addon-themes": "^10.2.4",
    "@storybook/react-vite": "^10.2.4",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@ugrc/eslint-config": "^1.2.3",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.26.0",
    "firebase-tools": "^15.4.0",
    "fireway": "^1.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.22",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prop-types": "^15.8.1",
    "storybook": "^10.2.13",
    "tailwindcss": "^3.4.19",
    "tailwindcss-react-aria-components": "^1.2.0",
    "vite": "^7.3.1",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^4.0.14",
    "wait-on": "^9.0.3"
  }
}
