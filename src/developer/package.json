{
  "name": "@ugrc/developer",
  "version": "2.7.1",
  "private": true,
  "description": "The API key self service website for the UGRC API.",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:storybook": "storybook build --output-dir ./dist/storybook",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:firebase\" \"wait-on http://127.0.0.1:4000 && npm run dev:vite\"",
    "dev:firebase": "firebase emulators:start --import ../../data/firebase --only auth,firestore,functions",
    "dev:vite": "vite",
    "firebase:migrate:dev": "fireway migrate --projectId ut-dts-agrc-web-api-dev",
    "firebase:migrate:local": "FIRESTORE_EMULATOR_HOST='127.0.0.1:8080' fireway migrate --projectId ut-dts-agrc-web-api-dev",
    "firebase:migrate:prod": "fireway migrate --projectId ut-dts-agrc-web-api-prod",
    "firebase:save-state": "firebase emulators:start --import ../../data/firebase --export-on-exit --only auth,firestore,functions",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint ./ --ext .js,.jsx --quiet --fix --ignore-path ./.eslintignore",
    "preview": "vite preview",
    "start": "npm run dev",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --silent --coverage --ui --open"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@storybook/test": "^8.6.14",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-table": "^8.21.3",
    "@ugrc/utah-design-system": "^1.27.1",
    "@vitejs/plugin-react": "^4.6.0",
    "clsx": "^2.1.1",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.3.2",
    "md5": "^2.3.0",
    "react": "^19.1.0",
    "react-aria-components": "^1.10.1",
    "react-copy-to-clipboard-ts": "^1.3.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.59.0",
    "react-router": "^7.6.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@ugrc/eslint-config": "^1.2.2",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.1.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "firebase-tools": "^14.9.0",
    "fireway": "^1.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.17",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "prop-types": "^15.8.1",
    "storybook": "^8.6.12",
    "tailwindcss": "^3.4.17",
    "tailwindcss-react-aria-components": "^1.2.0",
    "vite": "^6.3.5",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^3.1.3",
    "wait-on": "^8.0.3"
  }
}
