{
  "name": "@ugrc/developer",
  "version": "2.7.0",
  "private": true,
  "description": "The API key self service website for the UGRC API.",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:storybook": "storybook build --output-dir ./dist/storybook",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:firebase\" \"wait-on http://127.0.0.1:4000 && npm run dev:vite\"",
    "dev:firebase": "firebase emulators:start --import ../../data/firebase --only auth,firestore,functions",
    "dev:vite": "vite",
    "firebase:migrate:dev": "fireway migrate --projectId ut-dts-agrc-web-api-dev",
    "firebase:migrate:local": "FIRESTORE_EMULATOR_HOST='127.0.0.1:8080' fireway migrate --projectId ut-dts-agrc-web-api-dev",
    "firebase:migrate:prod": "fireway migrate --projectId ut-dts-agrc-web-api-prod",
    "firebase:save-state": "firebase emulators:start --import ../../data/firebase --export-on-exit --only auth,firestore,functions",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint ./ --ext .js,.jsx --quiet --fix --ignore-path ./.eslintignore",
    "preview": "vite preview",
    "start": "npm run dev",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --silent --coverage --ui --open"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-navigation-menu": "^1.2.4",
    "@storybook/test": "^8.5.0",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-table": "^8.20.6",
    "@ugrc/utah-design-system": "^1.19.0",
    "@vitejs/plugin-react": "^4.3.4",
    "clsx": "^2.1.1",
    "firebase": "^11.2.0",
    "firebase-admin": "^13.0.2",
    "firebase-functions": "^6.2.0",
    "md5": "^2.3.0",
    "react": "^18.3.1",
    "react-aria-components": "^1.6.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.54.2",
    "react-router": "^7.1.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-links": "^8.5.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^8.5.0",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@tanstack/react-query-devtools": "^5.64.2",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^3.0.3",
    "@vitest/ui": "^3.0.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.11.2",
    "firebase-tools": "^13.29.1",
    "fireway": "^1.1.0",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.8",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "prop-types": "^15.8.1",
    "storybook": "^8.5.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-react-aria-components": "^1.2.0",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.11",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^3.0.3",
    "wait-on": "^8.0.2"
  }
}
