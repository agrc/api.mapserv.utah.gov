{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Level:u3} {Timestamp:HH:mm:ss}] ({SourceContext:l}) {Message:lj} {NewLine}{Exception}"
        }
      }
    ],
    "Using": ["Serilog.Sinks.Console"],
    "Enrich": ["FromLogContext"]
  },
  "AllowedHosts": "*",
  "webapi": {
    "arcgis": {
      "host": "localhost",
      "port": 6080,
      "geometryService": {
        "path": "arcgis/rest/services/Geometry/GeometryServer"
      }
    },
    "locators": [{
      "host": "localhost",
      "displayName": "Centerlines.StatewideRoads",
      "serviceName": "Roads_AddressSystem_STREET",
      "reverseGeocodes": true,
      "locatorType": "RoadCenterlines",
      "weight": 0,
      "pathToLocator": "/arcgis/rest/services/Geolocators/"
    }, {
      "host": "localhost",
      "displayName": "AddressPoints.AddressGrid",
      "serviceName": "AddressPoints_AddressSystem",
      "reverseGeocodes": false,
      "locatorType": "AddressPoints",
      "weight": 1,
      "pathToLocator": "/arcgis/rest/services/Geolocators/"
    }],
    "database": {
      "host": "localhost",
      "port": 5432,
      "db": "webapi",
      "username": "postgres",
      "password": "what password",
      "pepper": "spicy"
    }
  }
}
